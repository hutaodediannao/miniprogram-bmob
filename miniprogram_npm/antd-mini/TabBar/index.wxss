.ant-tab-bar-wrap {
  display: flex;
  align-items: center;
}
.ant-tab-bar-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 4px 0;
  color: #666666;
}
.ant-tab-bar-item-active {
  color: white;
  font-weight: bold;
}
.ant-tab-bar-icon {
  max-width: 24px;
  max-height: 24px;
  font-size: 24px;
}
.ant-tab-bar-image {
  font-size: 24px;
  max-width: 24px;
  max-height: 24px;
}
.ant-tab-bar-text {
  font-size: 15px;
}
